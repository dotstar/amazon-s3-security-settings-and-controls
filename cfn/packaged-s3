AWSTemplateFormatVersion: '2010-09-09'
Description: S3 lab - includes VPC resources.
Parameters:
  BucketName:
    Type: String
    Description: Name of bucket to use for this lab
    Default: daveaws-tmp-xyzzy
    AllowedPattern: ^[a-zA-Z0-9.\-_]{1,255}$
Resources:
  VPCStack:
    Type: AWS::CloudFormation::Stack
    Properties:
      Tags:
      - Key: Project
        Value: lab
      TemplateURL: https://s3.us-east-2.amazonaws.com/daveaws/cloudformation/s3/1567414308be2d47a52c47bf1b8f3718.template
      TimeoutInMinutes: 5
      Parameters:
        EnvironmentName: S3VPC
        VpcCIDR: 10.2.0.0/16
        PublicSubnet1CIDR: 10.2.1.0/24
        IsEventEngine: 'false'
  S3Stack:
    Type: AWS::CloudFormation::Stack
    Properties:
      Tags:
      - Key: Project
        Value: lab
      TemplateURL: https://s3.us-east-2.amazonaws.com/daveaws/cloudformation/s3/78c93904357a883ace7bd74596def8c8.template
      TimeoutInMinutes: 5
      Parameters:
        BucketName:
          Ref: BucketName
